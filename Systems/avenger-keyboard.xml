<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>
  
  <keyboard>
    
    <key n="68">
      <name>D</name>
      <desc>Bomber bay</desc>
      <binding>
        <command>nasal</command>
        <script>doors.passenger.toggle();</script>
      </binding>
    </key>
    
    <key n="100">
      <name>d</name>
      <desc>Crew door</desc>
      <binding>
        <command>nasal</command>
        <script>doors.crew.toggle();</script>
      </binding>
    </key>
    
    <key n="69">
      <name>E</name>
      <desc>Wings</desc>
      <binding>
        <command>nasal</command>
        <script>doors.wings.toggle();</script>
      </binding>
    </key>
    
